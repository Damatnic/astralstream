{
  "name": "astralstream",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:android": "eas build -p android --profile production",
    "build:android-local": "npx expo prebuild -p android --clean && cd android && gradlew.bat assembleRelease",
    "build:android-local-unix": "npx expo prebuild -p android --clean && cd android && ./gradlew assembleRelease",
    "prebuild": "npx expo prebuild --clean",
    "postinstall": "npx expo install --fix"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.0",
    "expo-av": "~13.4.1",
    "expo-clipboard": "^7.1.5",
    "expo-file-system": "~15.4.5",
    "expo-keep-awake": "~12.3.0",
    "expo-media-library": "~15.4.1",
    "expo-screen-orientation": "~6.0.6",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
